{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Run test.c",
          "preLaunchTask": "Compile test.c",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/test.out",
          "args": ["20"],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}/",
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ]
      },
  ]
}